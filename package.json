{
    "name": "spa-template-react",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "start": "vite --host",
        "test": "vitest",
        "test-stage": "vitest run ./src",
        "test:coverage": "vitest run --coverage",
        "test:coverage:build": "vitest run --coverage",
        "test:coverage:ui": "vitest run --coverage && open coverage/index.html",
        "build": "tsc -b && vite build",
        "build:analyze": "vite build --mode analyze",
        "lint": "eslint ./src",
        "prettify": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
        "preview": "vite preview",
        "prepare": "husky",
        "lint-stage": "lint-staged",
        "cr:feature": "lekalo gen feature path=./src/3_features",
        "cr:page": "lekalo gen page path=./src/2_pages",
        "clean": "rm -rf node_modules"
    },
    "dependencies": {
        "@types/node": "20.19.26",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@types/react-router-dom": "5.3.3",
        "@vitejs/plugin-react": "4.3.4",
        "classnames": "2.5.1",
        "dayjs": "1.11.13",
        "dotenv": "16.5.0",
        "husky": "9.1.7",
        "i18next": "25.3.2",
        "keycloak-js": "26.1.5",
        "mobx": "6.13.7",
        "mobx-react-lite": "4.1.0",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-router-dom": "5.3.4",
        "typescript": "5.9.3",
        "uuid": "11.1.0",
        "vite": "6.4.1",
        "yup": "1.6.1"
    },
    "devDependencies": {
        "@eslint/js": "9.21.0",
        "@vitest/coverage-v8": "3.2.1",
        "eslint": "9.39.1",
        "eslint-plugin-import-firewall": "1.0.1",
        "eslint-plugin-postcss-modules": "2.0.0",
        "eslint-plugin-prettier": "5.2.6",
        "eslint-plugin-react-hooks": "5.1.0",
        "eslint-plugin-react-refresh": "0.4.19",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "globals": "15.15.0",
        "jsdom": "26.1.0",
        "lekalo": "0.0.2",
        "lint-staged": "15.5.1",
        "prettier": "3.5.3",
        "rollup-plugin-visualizer": "6.0.5",
        "tslib": "2.8.1",
        "typescript-eslint": "8.49.0",
        "vitest": "3.2.1"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "prettier --write",
            "eslint"
        ]
    }
}
